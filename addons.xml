<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.RetoForteKodiWizard" name="RetoForte" version="1.0.0" provider-name="RetoForte">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Reto Forte Wizard</summary>
    <description lang="en">Tu instalador de proveedores de contenido. Nos actualizamos constantemente para mantener que tu experiencia de entretenimiento sea mejor.</description>
	<website>http://google.com</website>
    <platform>all</platform>
  </extension>
</addon>


<addon id="repository.retoforte" name="RetoForte official Repo" version="1.0.0" provider-name="RetoForte">
	<extension point="xbmc.addon.repository" name="RetoForte official Repo">
		<info compressed="false">https://raw.githubusercontent.com/retoforte/RetoForte/master/repository.retoforte/addon.xml</info>
		<checksum>https://raw.githubusercontent.com/retoforte/RetoForte/master/repository.retoforte/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/retoforte/RetoForte/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>RetoForte official Repo</summary>
   		<description lang="en">Mantiene actualizaciones de tu Paquete de Canales de Contenido</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
